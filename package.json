{
  "name": "completed-games-tracker",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5173",
    "electron": "npm --prefix electron run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run electron\"",
    "build": "rm -rf electron/dist && vite build && tsc -p electron && npm --prefix electron run start"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.2.2",
    "vite": "^7.2.2"
  }
}
